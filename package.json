{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar-scanner:scan": "sonar-scanner -Dsonar.login=$SONAR_LOGIN -Dsonar.password=$SONAR_PASSWORD",
    "prepare": "husky install && chmod ug+x .husky/* && chmod ug+x .git/hooks/*",
    "lint:format": "npx --no-install lint-staged && npx --no-install pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/cdk": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/material": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "bootstrap": "^4.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jasmine": "^3.6.11",
    "@types/node": "^14.17.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-jsdoc": "^34.7.0",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-rxjs": "^3.3.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "sonarqube-scanner": "^2.8.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --fix"
    ]
  }
}
